@misc{wisemanFurtherEvidenceThat2023,
  title = {Further Evidence That Galaxy Age Drives Observed Type {{Ia}} Supernova Luminosity Differences},
  author = {Wiseman, P. and Sullivan, M. and Smith, M. and Popovic, B.},
  year = {2023},

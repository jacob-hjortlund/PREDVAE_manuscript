@article{smithSDSSIISUPERNOVASURVEY2012,
  title = {{{THE SDSS-II SUPERNOVA SURVEY}}: {{PARAMETERIZING THE TYPE Ia SUPERNOVA RATE AS A FUNCTION OF HOST GALAXY PROPERTIES}}},

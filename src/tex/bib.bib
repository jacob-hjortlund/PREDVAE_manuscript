@misc{fortunatoCosmographyWelllocalizedFast2023,
  title = {Cosmography from Well-Localized {{Fast Radio Bursts}}},
  author = {Fortunato, J{\'e}ferson A. S. and {Hip{\'o}lito-Ricaldi}, Wiliam S. and dos Santos, Marcelo V.},
  year = {2023},

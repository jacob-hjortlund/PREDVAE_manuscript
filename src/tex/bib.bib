@article{hergtBayesianEvidenceTensortoscalar2021,
  title = {Bayesian Evidence for the Tensor-to-Scalar Ratio \$r\$ and Neutrino Masses \$m\_\textbackslash nu\$: {{Effects}} of Uniform vs Logarithmic Priors},
